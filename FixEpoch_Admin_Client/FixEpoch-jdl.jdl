entity Admin {
	adminName String
	email String
	password String
	phNumber String

}

entity Catagorie {
	catagorieName String
	details String

}

entity Firm {
	actorName String
	phNumber String
}

entity Character {
	firmName String
	phNumber String
	adress String
	email String
	time String
    	}

entity User {
	userName String
	password String

}

relationship OneToMany {
	Catagorie to Firm{catagorie},
	Admin to Catagorie{admin}
	}
}
