entity Admin {
	adminName String
	email String
	password String
	phNumber String

}

entity Category {
	categoryName String
	details String

}

entity Firm {
	firmName String
	phNumber String
	adress String
	email String
	time String
    	}

entity User {
	userName String
	password String

}

relationship OneToMany {
	Admin to Category{admin},
	Category to Firm{category},

	}
